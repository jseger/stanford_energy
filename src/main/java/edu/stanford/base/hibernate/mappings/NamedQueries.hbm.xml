<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <sql-query name="edu.stanford.base.hibernate.mappings.getTotalEmployeeCount">
        <![CDATA[
           SELECT *  FROM products  where price >0 order by productId asc
            ]]>
    </sql-query>
    
</hibernate-mapping>
